import java.util.ArrayList;

public class PasswordUnitTest {
	

	public static void main(String[] args) {
		ArrayList<String> passwordStorage = new ArrayList<String>();
		addPassword(passwordStorage,"joemama");
	}

	public static boolean addPassword(ArrayList<String> passwordStorage, String password) {
		if (!passwordStorage.contains(password)) {
			passwordStorage.add(password);
			return true;
		}
		System.out.println(passwordStorage);
		
		return false;
	}
	
//	public static ArrayList<String> passwordStorage(String acceptedPassword){
